{
  "expo": {
    "name": "PoketTool",
    "slug": "PoketTool",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon3.png",
    "newArchEnabled": true,
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/icon3.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon3.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.empanada.PoketTool",
      "version": "4"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6b81ba23-aae7-4420-94b5-0a0d5062165b"
      }
    }
  }
}
